    "commonjs": true,
    "es2021": true,
    "node": true,
    "jest/globals": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "plugins": ["jest"],
  "rules": {
    "no-console": "off",
    "linebreak-style": ["error", "unix"]
  }
}
